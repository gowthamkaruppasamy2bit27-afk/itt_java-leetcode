bool isValid(char* s)
 {
    int c=0,i,j=-1;
    int st[1000000];
    for(i=0;i<strlen(s);i++)
    {
        if(s[i]=='(' || s[i] =='{'|| s[i]=='[')
        {
            j+=1;
            st[j]=s[i];
        }
        else
        {
            if(j==-1)
            {
                j=0;
                st[j]=0;
            }
            if((st[j]=='(' && s[i]==')')||(st[j]=='{'&& s[i]=='}')||(st[j]=='['&& s[i]==']'))
            {
                j--;
            }
            else
            {
                break;
            }
        }
    }
        if(j!=-1)
        {
            return false;
        }
    return true;
}
